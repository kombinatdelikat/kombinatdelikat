        <source>Enclosures</source>
        <target>Fişiere</target>
      </trans-unit>
      <trans-unit id="tl_news.source_legend">
        <source>Redirect target</source>
        <target>Redirecţionare</target>
      </trans-unit>
      <trans-unit id="tl_news.expert_legend">
        <source>Expert settings</source>
        <target>Setări avansate</target>
      </trans-unit>
      <trans-unit id="tl_news.publish_legend">
        <source>Publish settings</source>
        <target>Publicare</target>
      </trans-unit>
      <trans-unit id="tl_news.new.0">
        <source>New article</source>
        <target>Anunţ nou</target>
      </trans-unit>
      <trans-unit id="tl_news.new.1">
        <source>Create a new news article</source>
        <target>Creează un anunţ nou</target>
      </trans-unit>
      <trans-unit id="tl_news.show.0">
        <source>Article details</source>
        <target>Detalii anunţ</target>
      </trans-unit>
      <trans-unit id="tl_news.show.1">
        <source>Show the details of news article ID %s</source>
        <target>Arată detaliile anunţului ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.edit.0">
        <source>Edit article</source>
        <target>Editează anunţ</target>
      </trans-unit>
      <trans-unit id="tl_news.edit.1">
        <source>Edit news article ID %s</source>
        <target>Editează anunţul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.copy.0">
        <source>Duplicate article</source>
        <target>Copiază anunţ</target>
      </trans-unit>
      <trans-unit id="tl_news.copy.1">
        <source>Duplicate news article ID %s</source>
        <target>Copiază anunţul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.cut.0">
        <source>Move article</source>
        <target>Mută anunţ</target>
      </trans-unit>
      <trans-unit id="tl_news.cut.1">
        <source>Move news article ID %s</source>
        <target>Mută anunţ ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.delete.0">
        <source>Delete article</source>
        <target>Şterge anunţ</target>
      </trans-unit>
      <trans-unit id="tl_news.delete.1">
        <source>Delete news article ID %s</source>
        <target>Şterge anunţul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.toggle.0">
        <source>Publish/unpublish article</source>
        <target>Publică anunţ</target>
      </trans-unit>
      <trans-unit id="tl_news.toggle.1">
        <source>Publish/unpublish news article ID %s</source>
        <target>Publică anunţul ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.feature.0">
        <source>Feature/unfeature article</source>
        <target>Publică articol special</target>
      </trans-unit>
      <trans-unit id="tl_news.feature.1">
        <source>Feature/unfeature news article ID %s</source>
        <target>Publică articol special ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news.editheader.0">
        <source>Edit archive settings</source>
        <target>Editează setări arhivă</target>
      </trans-unit>
      <trans-unit id="tl_news.editheader.1">
        <source>Edit the archive settings</source>
        <target>Editează setările acestei arhive</target>
      </trans-unit>
      <trans-unit id="tl_news.editmeta.0">
        <source>Edit article settings</source>
        <target>Setări articol</target>
      </trans-unit>
      <trans-unit id="tl_news.editmeta.1">
        <source>Edit the article settings</source>
        <target>Editează setările articolului</target>
      </trans-unit>
      <trans-unit id="tl_news.pasteafter.0">
        <source>Paste into this archive</source>
        <target>Copiază în arhivă</target>
      </trans-unit>
      <trans-unit id="tl_news.pasteafter.1">
        <source>Paste after news article ID %s</source>
        <target>Copiază după anunţul ID %s</target>
      </trans-unit>
